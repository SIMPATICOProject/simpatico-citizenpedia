{
  "aggregate": "diagram",
  "pipeline": [
    {
      "$match": {
        "rootId": "{id}"
      }
    },
    {
      "$lookup": {
        "from": "model",
        "localField": "modelId",
        "foreignField": "_id",
        "as": "model"
      }
    },
    {
      "$unwind": "$model"
    },
    {
      "$match": {
        "model.eServiceId": {
          "$exists": true
        }
      }
    },
    {
      "$group": {
        "_id": "$rootId",
        "count": {
          "$sum": 1
        }
      }
    }
  ]
}
